<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="DiplomProject.App"
             xmlns:local="using:DiplomProject"
             xmlns:vm="using:DiplomProject.ViewModels"
             xmlns:views="using:DiplomProject.Views">
	<Application.DataTemplates>
		<DataTemplate DataType="vm:DashboardViewModel">
			<views:DashboardView/>
		</DataTemplate>
		<DataTemplate DataType="vm:OrdersViewModel">
			<views:OrdersView/>
		</DataTemplate>
	</Application.DataTemplates>
	<Application.Styles>
		<FluentTheme />
	</Application.Styles>
</Application>
